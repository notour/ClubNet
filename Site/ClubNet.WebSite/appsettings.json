{
  "ConnectionStrings": {
    "ClubNetWebSiteContextConnection": "Server=(localdb)\\mssqllocaldb;Database=ClubNet.WebSite;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "DefaultConfig": {
    "Theme": "Kronak"
  },
  "AllowedHosts": "*",
  "MongoDB": {
    "CollectionNames": {
      "Users": [
        "ClubNet.WebSite.Domain.User.UserInfo, ClubNet.WebSite.Domain"
      ],
      "Logs": [
        "ClubNet.WebSite.Domain.Logs.LogBase, ClubNet.WebSite.Domain",
        "ClubNet.WebSite.Domain.Logs.ErrorLog, ClubNet.WebSite.Domain"
      ],
      "Security": [
        "ClubNet.WebSite.Domain.Security.SecurityUser, ClubNet.WebSite.Domain",
        "ClubNet.WebSite.Domain.Security.SecurityRight, ClubNet.WebSite.Domain",
        "ClubNet.WebSite.Domain.Security.SecurityRightGroup, ClubNet.WebSite.Domain",
        "ClubNet.WebSite.Domain.Security.SecurityGroup, ClubNet.WebSite.Domain"
      ],
      "Configs": [
        "ClubNet.WebSite.Domain.Configs.GlobalConfig, ClubNet.WebSite.Domain"
      ]
    },
    "Host": "127.0.0.1",
    "DataBase": "ClubNet-Dev"
  }
}